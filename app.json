{
  "expo": {
    "name": "selftalk",
    "slug": "selftalk",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swag31415.selftalk",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to allow you to upload images and customize your profile."
      },
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.swag31415.selftalk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "076fcb70-d45b-450a-ad54-30bba382d3bf"
      }
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
